{
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "src/*": {
            "runtime": "@vercel/node"
        }
    }
}