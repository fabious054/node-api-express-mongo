{
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "api/*": {
            "runtime": "@vercel/node",
            "memory": 1024,
            "maxDuration": 300,
            "includeFiles": "./*"
        }
    }
}