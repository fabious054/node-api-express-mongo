{
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "./server.js": {
            "runtime": "@vercel/node",
            "includeFiles": "src/**",
            "excludeFiles": ["node_modules/**"],
            "maxDuration": 300,
            "memory": 1024
        }
    }
}