{
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node",
            "config": { "maxDuration": 300}
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "*": {
            "memory": 1,
            "maxDuration": 1
        }
    }
}