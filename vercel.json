{
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "server.js"
        }
    ],
    "functions": {
        "./server.js": {
          "runtime": "@vercel/node",
          "maxDuration": 100,
          "memory": 1024
        }
    }
}